{
    "version": 2,
    "public": true,
    "headers": [
        {
            "source": "/origin_Medical.svg",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "image/svg+xml"
                }
            ]
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/" }
    ]
}